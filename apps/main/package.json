{
  "name": "@ayp/main",
  "version": "1.11.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "predev": "rm -rf .next",
    "dev": "next dev",
    "predev:instrumented": "pnpm predev",
    "dev:instrumented": "INSTRUMENT_CODE=1 NEXT_PUBLIC_STAGE=testing NEXT_PUBLIC_GATEWAY_HOST=http://127.0.0.1:3100 NEXT_PUBLIC_PEOPLE_API_HOST=http://127.0.0.1:3100 NEXT_PUBLIC_FINTECH_API_HOST=http://127.0.0.1:3101 pnpm dev",
    "prebuild": "rm -rf .next",
    "build": "next build",
    "build:ci": "INSTRUMENT_CODE=1 pnpm build",
    "start": "next start -p ${PORT:=3000}",
    "lint": "next lint",
    "test:cy": "cypress run",
    "test:jest": "jest",
    "test": "pnpm test:jest && pnpm test:cy",
    "pretest": "rm -rf .nyc_output coverage",
    "posttest": "pnpm report:combined",
    "combine:reports": "jq -s '.[0] * .[1]' coverage-cypress/coverage-final.json coverage-jest/coverage-final.json > .nyc_output/out.json",
    "prereport:combined": "pnpm combine:reports",
    "report:combined": "pnpx nyc report --reporter html --report-dir coverage --nycrc-path .nycrc.coverage"
  },
  "dependencies": {
